{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "node-sass": "^4.9.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "watch:scss": "sass --watch src/styles/scss:src/styles/css",
    "start": "npm run watch:scss | react-scripts start",
    "build:scss": "sass src/styles/scss:src/styles/css",
    "build": "npm run build:scss && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
